{
  "name": "frostrnbase",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:clean": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "clean": "npm run clean:android && npm run clean:ios",
    "preios": "npm run sagas",
    "preandroid": "npm run sagas",
    "prestart": "npm run set-git-hash && npm run sagas",
    "prebuild": "npm run set-git-hash && npm run sagas",
    "set-git-hash": "node scripts/git-hash.js",
    "set-env": "node scripts/set-env.js",
    "postinstall": "npm run sagas",
    "sagas": "node scripts/find-sagas.js \"**/*.stories.tsx\" sagobok/sagas.ts"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.8",
